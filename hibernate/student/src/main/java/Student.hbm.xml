<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<?xml-model type="application/xml" schematypens="http://www.w3.org/2001/XMLSchema"?>
   <hibernate-mapping>
       <class name = "com.hibernate.student.Student" table = "Student_table">
         <id name = "StudentRollNo" column = "rollno"/>
         <property name = "StudentName" column="name"/>
         <property name = "StudentPhone" column="phone"/>
       </class>
      
   </hibernate-mapping>